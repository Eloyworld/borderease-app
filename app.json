{
  "expo": {
    "name": "Beyond Borders",
    "slug": "beyond-borders",
    "version": "1.0.2",
    "orientation": "portrait",
    "icon": "./assets/images/logo.png",
    "scheme": "bordereaseapp",
    "userInterfaceStyle": "automatic",
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.eloyworld.beyondborders",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      },
      "runtimeVersion": {
        "policy": "appVersion"
      }
    },
    "android": {
      "package": "com.anonymous.beyondborders",
      "runtimeVersion": "1.0.0"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "741bbabc-459d-4ffe-8ae5-7c0ab59113cd"
      }
    },
    "updates": {
      "url": "https://u.expo.dev/741bbabc-459d-4ffe-8ae5-7c0ab59113cd"
    }
  }
}
